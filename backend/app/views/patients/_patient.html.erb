<div id="<%= dom_id patient %>">
  <p>
    <strong>Name:</strong>
    <%= patient.name %>
  </p>

  <p>
    <strong>Surname:</strong>
    <%= patient.surname %>
  </p>

  <p>
    <strong>Username:</strong>
    <%= patient.username %>
  </p>

  <p>
    <strong>Password hash:</strong>
    <%= patient.password_hash %>
  </p>

</div>
